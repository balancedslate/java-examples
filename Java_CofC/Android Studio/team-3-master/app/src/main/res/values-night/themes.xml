<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Kipos" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/dark_brown</item>
        <item name="colorPrimaryVariant">@color/pastel_greenC</item>
        <item name="colorOnPrimary">@color/dark_green</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/mid_green</item>
        <item name="colorSecondaryVariant">@color/dark_brown</item>
        <item name="colorOnSecondary">@color/light_brown</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
</resources>